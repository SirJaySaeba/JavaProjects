<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
   <bean id="calc" class="com.tutorialspoint.CalcArea">
   </bean>

	<bean id="tri" class="com.tutorialspoint.Triangle">
		<property name="laenge" value="20"></property>
		<property name="hoehe" value="10"></property>
	</bean>
	<bean id="rec" class="com.tutorialspoint.Rectangle">
		<property name="laenge" value="20"></property>
		<property name="breite" value="10"></property>
	</bean>
	<bean id="cir" class="com.tutorialspoint.Circle">
		<property name="radius" value="20"></property>
	</bean>	
</beans>